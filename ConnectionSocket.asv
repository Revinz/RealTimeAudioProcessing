classdef (Abstract) ConnectionSocket < Interactable
    properties 
        connectionLine;
        node;
        socketOffset;
        socketSize = [.03 .05]; %The size of the sockets
    end
    
    methods
        function obj = updateSocketOffset(obj)
            obj.socketOffset = [(obj.socketSize(1)/2) (obj.socketSize(2)/2)];
        end
        
    end
end